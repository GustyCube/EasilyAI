[run]
source = easilyai
omit = 
    */tests/*
    */_version.py
    */test_*.py
    */conftest.py
    */__pycache__/*
    */venv/*
    */.venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    if typing.TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod
    except ImportError:
    pass
    ...

precision = 2
show_missing = True
skip_covered = False
skip_empty = True

[html]
directory = htmlcov
title = EasilyAI Coverage Report

[xml]
output = coverage.xml